{
  "name": "fleet-management-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run lint && npm run check-types && npm run build",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@popperjs/core": "^2.11.0",
    "@tailwindcss/forms": "^0.3.4",
    "graphql": "^16.0.1",
    "js-cookie": "^3.0.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-datepicker": "^4.4.0",
    "react-dom": "17.0.2",
    "react-popper": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/fragment-matcher": "3.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.0",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.1",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/react": "17.0.37",
    "@types/react-datepicker": "^4.3.2",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.5.0",
    "tailwindcss": "^2.2.19",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  }
}
